<h2>お問い合わせ画面</h2>

<%= form_with(model: @blog, local: true) do |form| %> 
  <div class="contact_title">
    <%= form.label :Name %>
    <%= form.text_field :Name %>
  </div>
  <div class="contact_title">
    <%= form.label :Email %>
    <%= form.email_field :Email %>
  </div>
  <div class="contact_title">
    <%= form.label :content %>
    <%= form.text_field :content%>
  </div>
  <%= form.submit %>
<% end %>